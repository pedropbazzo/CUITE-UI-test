<UserControl
    x:Class="Sut.Wpf.Controls.Controls.TreeViewControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <TreeView
        AutomationProperties.AutomationId="KDe9Dr8_kEuy9PZl5onvGQ"
        Style="{DynamicResource DefaultControlStyle}">
        <TreeViewItem Header="C:">
            <TreeViewItem Header="Program Files" />
            <TreeViewItem Header="Program Files (x86)" />
            <TreeViewItem Header="ProgramData" />
            <TreeViewItem Header="Users" />
            <TreeViewItem Header="Windows" />
        </TreeViewItem>
    </TreeView>

</UserControl>